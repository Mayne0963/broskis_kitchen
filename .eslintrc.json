{
  "extends": ["next/core-web-vitals"],
  "rules": {
    "no-redeclare": "error",
    "no-duplicate-imports": "error",
    "no-unused-vars": "off",
    "prefer-const": "error",
    "import/no-anonymous-default-export": "off"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}